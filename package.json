{
	"name": "patternlibraryexample",
	"description": "pattern library project using DaVanMonet",
	"version": "1.1.3",
	"license": "UNLICENSED",
	"private": true,
	"scripts": {
		"dev": "node node_modules/davanmonet/dvm-build/dev-server.js",
		"build-dvm": "node node_modules/davanmonet/dvm-build/build-dvm.js",
		"build-pl": "node node_modules/davanmonet/dvm-build/build-patternlibrary.js",
		"build-all": "npm run build-dvm && npm run build-pl",
		"build": "npm run build-all",
		"dev-debug": "node --inspect ../DaVanMonet/dvm-build/dev-server.js"
	},
	"devDependencies": {
		"davanmonet": "^1.1.5",
		"grunt": "1.0.1",
		"grunt-run": "^0.8.0",
		"stylelint": "^8.3.1",
		"stylelint-config-standard": "^18.0.0",
		"stylelint-webpack-plugin": "^0.9.0"
	},
	"config": {
		"configFile": "./configs/projectoptions.yml",
		"webpackConfig": "./configs/webpack.conf.js"
	},
	"babel": {
		"presets": [
			"stage-2"
		]
	},
	"dependencies": {
		"include-media": "^1.4.9"
	}
}
